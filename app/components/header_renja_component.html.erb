<tr class="<%= style_header %> nobreak always">
  <th class="border" scope="col" rowspan="2">Kode <%= @title %></th>
  <th class="border" scope="col" rowspan="2" colspan="3">Nama <%= @title %></th>
  <% @periode.each do |tahun| %>
    <th class="border text-center" scope="col" colspan="5"><%= tahun %></th>
  <% end %>
</tr>
<tr class="<%= style_header %> nobreak">
  <% @periode.each do %>
    <% if @indikator %>
      <th class="border" scope="col">Indikator</th>
      <th class="border" scope="col" width="50px">Target</th>
      <th class="border" scope="col">Satuan</th>
    <% else%>
      <th class="border" scope="col" colspan="3"></th>
    <% end %>
    <th class="border" scope="col" colspan="2">Pagu Indikatif</th>
  <% end %>
</tr>