<%= form_with(scope: :anggaran_sbus, url: anggaran_sbus_index_path ) do |form| %>
  <div class="field">
    <%= form.label :kode_kelompok_barang %>
    <%= form.text_field :kode_kelompok_barang %>
  </div>
  <div class="field">
    <%= form.label :uraian_kelompok_barang %>
    <%= form.text_field :uraian_kelompok_barang %>
  </div>
  <div class="field">
    <%= form.label :kode_barang %>
    <%= form.text_field :kode_barang %>
  </div>
  <div class="field">
    <%= form.label :uraian_barang %>
    <%= form.text_field :uraian_barang %>
  </div>
  <div class="field">
    <%= form.label :spesifikasi %>
    <%= form.text_field :spesifikasi %>
  </div>
  <div class="field">
    <%= form.label :satuan %>
    <%= form.text_field :satuan %>
  </div>
  <div class="field">
    <%= form.label :harga_satuan %>
    <%= form.text_field :harga_satuan %>
  </div>
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
