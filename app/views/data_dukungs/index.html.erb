<p id="notice"><%= notice %></p>

<h1>Data Dukungs</h1>

<table>
  <thead>
    <tr>
      <th>Nama data</th>
      <th>Keterangan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_dukungs.each do |data_dukung| %>
      <tr>
        <td><%= data_dukung.nama_data %></td>
        <td><%= data_dukung.keterangan %></td>
        <td><%= link_to 'Show', data_dukung %></td>
        <td><%= link_to 'Edit', edit_data_dukung_path(data_dukung) %></td>
        <td><%= link_to 'Destroy', data_dukung, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Dukung', new_data_dukung_path %>
