<p id="notice"><%= notice %></p>

<h1>External Urls</h1>

<table>
  <thead>
    <tr>
      <th>Aplikasi</th>
      <th>Endpoint</th>
      <th>Username</th>
      <th>Password</th>
      <th>Keterangan</th>
      <th>Kode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @external_urls.each do |external_url| %>
      <tr>
        <td><%= external_url.aplikasi %></td>
        <td><%= external_url.endpoint %></td>
        <td><%= external_url.username %></td>
        <td><%= external_url.password %></td>
        <td><%= external_url.keterangan %></td>
        <td><%= external_url.kode %></td>
        <td><%= link_to 'Show', external_url %></td>
        <td><%= link_to 'Edit', edit_external_url_path(external_url) %></td>
        <td><%= link_to 'Destroy', external_url, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New External Url', new_external_url_path %>
