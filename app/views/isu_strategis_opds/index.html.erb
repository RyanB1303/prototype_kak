<p id="notice"><%= notice %></p>

<h1>Isu Strategis Opds</h1>

<table>
  <thead>
    <tr>
      <th>Kode</th>
      <th>Isu strategis</th>
      <th>Tahun</th>
      <th>Kode opd</th>
      <th>Tujuan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @isu_strategis_opds.each do |isu_strategis_opd| %>
      <tr>
        <td><%= isu_strategis_opd.kode %></td>
        <td><%= isu_strategis_opd.isu_strategis %></td>
        <td><%= isu_strategis_opd.tahun %></td>
        <td><%= isu_strategis_opd.kode_opd %></td>
        <td><%= isu_strategis_opd.tujuan %></td>
        <td><%= link_to 'Show', isu_strategis_opd %></td>
        <td><%= link_to 'Edit', edit_isu_strategis_opd_path(isu_strategis_opd) %></td>
        <td><%= link_to 'Destroy', isu_strategis_opd, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

