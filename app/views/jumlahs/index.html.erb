<p id="notice"><%= notice %></p>

<h1>Jumlahs</h1>

<table>
  <thead>
    <tr>
      <th>Jumlah</th>
      <th>Satuan</th>
      <th>Tahun</th>
      <th>Keterangan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jumlahs.each do |jumlah| %>
      <tr>
        <td><%= jumlah.jumlah %></td>
        <td><%= jumlah.satuan %></td>
        <td><%= jumlah.tahun %></td>
        <td><%= jumlah.keterangan %></td>
        <td><%= link_to 'Show', jumlah %></td>
        <td><%= link_to 'Edit', edit_jumlah_path(jumlah) %></td>
        <td><%= link_to 'Destroy', jumlah, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jumlah', new_jumlah_path %>
