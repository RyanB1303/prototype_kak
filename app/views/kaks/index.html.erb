<p id="notice"><%= notice %></p>

<h1>Kaks</h1>

<table>
  <thead>
    <tr>
      <th>Dasar hukum</th>
      <th>Tujuan</th>
      <th>Sasaran kinerja</th>
      <th>Indikator kinerja</th>
      <th>Target</th>
      <th>Satuan</th>
      <th>Penerima manfaat</th>
      <th>Data terpilah</th>
      <th>Akses</th>
      <th>Partisipasi</th>
      <th>Kontrol</th>
      <th>Manfaat</th>
      <th>Penyebab internal</th>
      <th>Penyebab external</th>
      <th>Permasalahan umum</th>
      <th>Permasalahan gender</th>
      <th>Resiko</th>
      <th>Lokasi pelaksanaan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kaks.each do |kak| %>
      <tr>
        <td><%= kak.dasar_hukum %></td>
        <td><%= kak.tujuan %></td>
        <td><%= kak.sasaran_kinerja %></td>
        <td><%= kak.indikator_kinerja %></td>
        <td><%= kak.target %></td>
        <td><%= kak.satuan %></td>
        <td><%= kak.penerima_manfaat %></td>
        <td><%= kak.data_terpilah %></td>
        <td><%= kak.akses %></td>
        <td><%= kak.partisipasi %></td>
        <td><%= kak.kontrol %></td>
        <td><%= kak.manfaat %></td>
        <td><%= kak.penyebab_internal %></td>
        <td><%= kak.penyebab_external %></td>
        <td><%= kak.permasalahan_umum %></td>
        <td><%= kak.permasalahan_gender %></td>
        <td><%= kak.resiko %></td>
        <td><%= kak.lokasi_pelaksanaan %></td>
        <td><%= link_to 'Show', kak %></td>
        <td><%= link_to 'Edit', edit_kak_path(kak) %></td>
        <td><%= link_to 'Destroy', kak, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kak', new_kak_path %>
