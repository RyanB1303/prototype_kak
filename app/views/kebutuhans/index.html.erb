<p id="notice"><%= notice %></p>

<h1>Kebutuhans</h1>

<table>
  <thead>
    <tr>
      <th>Kebutuhan</th>
      <th>Tahun</th>
      <th>Keterangan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kebutuhans.each do |kebutuhan| %>
      <tr>
        <td><%= kebutuhan.kebutuhan %></td>
        <td><%= kebutuhan.tahun %></td>
        <td><%= kebutuhan.keterangan %></td>
        <td><%= link_to 'Show', kebutuhan %></td>
        <td><%= link_to 'Edit', edit_kebutuhan_path(kebutuhan) %></td>
        <td><%= link_to 'Destroy', kebutuhan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kebutuhan', new_kebutuhan_path %>
