<p id="notice"><%= notice %></p>

<h1>Kesenjangans</h1>

<table>
  <thead>
    <tr>
      <th>Akses</th>
      <th>Partisipasi</th>
      <th>Kontrol</th>
      <th>Manfaat</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kesenjangans.each do |kesenjangan| %>
      <tr>
        <td><%= kesenjangan.akses %></td>
        <td><%= kesenjangan.partisipasi %></td>
        <td><%= kesenjangan.kontrol %></td>
        <td><%= kesenjangan.manfaat %></td>
        <td><%= link_to 'Show', kesenjangan %></td>
        <td><%= link_to 'Edit', edit_kesenjangan_path(kesenjangan) %></td>
        <td><%= link_to 'Destroy', kesenjangan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kesenjangan', new_kesenjangan_path %>
