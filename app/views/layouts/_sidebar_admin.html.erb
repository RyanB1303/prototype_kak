<li class="nav-item">
  <%= active_link_to(root_path, wrap_tag: :li, wrap_class: 'nav-item', active: :exact, class: "nav-link") do %>
    <span class="sidebar-icon">
      <i class="fas fa-chart-pie icon icon-xs me-2"></i>
    </span>
    <span class="sidebar-text">Dashboard</span>
  <% end %>
</li>
<li class="nav-item">
  <span class="nav-link d-flex
        justify-content-between align-items-center
        <%= collapsed_perencanaan_kota_items[:menu] %>"
        aria-expanded="<%= collapsed_perencanaan_kota_items[:aria] %>"
        data-bs-toggle="collapse" data-bs-target="#data-perencanaan-opd-items">
    <span>
      <span class="sidebar-text">Perencanaan OPD</span>
    </span>
    <span class="link-arrow">
      <i class="fas fa-angle-right icon icon-sm"></i>
    </span>
  </span>
  <div class="multi-level <%= collapsed_perencanaan_opd_items[:sub_menu] %>"
       aria-expanded="<%= collapsed_perencanaan_opd_items[:aria] %>"
       role="list" id="data-perencanaan-opd-items">
    <ul class="flex-column nav">
      <% perencanaan_opd_items.each do |item| %>
        <% if item[:multi] %>
          <li class="nav-item" >
            <span class="nav-link d-flex
                justify-content-between align-items-center
                <%= item[:collapse_items][:menu] %>"
                aria-expanded="<%= item[:collapse_items][:aria] %>"
                data-bs-toggle="collapse" data-bs-target="#<%= item[:id_target] %>">
              <span>
                <span class="sidebar-icon">
                  <i class="<%= item[:icon] %> icon icon-xs me-2"></i>
                </span>
                <span class="sidebar-text"><%= item[:title] %></span>
              </span>
              <span class="link-arrow">
                <i class="fas fa-angle-right icon icon-sm"></i>
              </span>
            </span>
            <div class="multi-level ms-3 <%= item[:collapse_items][:sub_menu] %>"
               aria-expanded="<%= item[:collapse_items][:aria] %>"
               role="list" id="<%= item[:id_target] %>">
              <ul class="flex-column nav">
                <% item[:collections].each do |sub_item| %>
                  <li class="nav-item" >
                    <%= active_link_to(sub_item[:href], wrap_tag: :li, wrap_class: 'nav-item',class: "nav-link") do %>
                      <span>
                        <span class="sidebar-text"><%= sub_item[:title] %></span>
                      </span>
                    <% end %>
                  </li>
                <% end %>
              </ul>
            </div>
          </li>
        <% else %>
          <li class="nav-item" >
            <%= active_link_to(item[:href], wrap_tag: :li, wrap_class: 'nav-item',class: "nav-link") do %>
              <span class="sidebar-icon">
                <i class="<%= item[:icon] %> icon icon-xs me-2"></i>
              </span>
              <span>
                <span class="sidebar-text"><%= item[:title] %></span>
              </span>
            <% end %>
          </li>
        <% end %>
      <% end %>
    </ul>
  </div>
</li>
<%# Renstra %>
<li class="nav-item">
  <%= active_link_to(renstra_index_path, wrap_tag: :li, wrap_class: 'nav-item',
                                 class: "nav-link d-flex justify-content-between align-items-center") do %>
    <span>
      <span class="sidebar-icon">
        <i class="fas fa-receipt icon-xs me-2"></i>
      </span>
      <span class="sidebar-text">Renstra</span>
    </span>
  <% end %>
</li>
<%# Renja %>
<li class="nav-item">
  <span class="nav-link d-flex justify-content-between align-items-center"
        data-bs-toggle="collapse" data-bs-target="#renja-items">
    <span>
      <span class="sidebar-icon">
        <i class="fas fa-thumbtack icon-xs me-2"></i>
      </span>
      <span class="sidebar-text">Renja</span>
    </span>
    <span class="link-arrow">
      <i class="fas fa-angle-right icon icon-sm"></i>
    </span>
  </span>
  <div class="multi-level" role="list" id="renja-items"
        aria-expanded="<%= collapsed_perencanaan_kota_items[:aria] %>">
    <ul class="flex-column nav">
      <% renja_items.each do |renja| %>
        <li class="nav-item" >
          <%= active_link_to(renja[:href], wrap_tag: :li, wrap_class: 'nav-item', class: "nav-link") do %>
            <span>
              <span class="sidebar-text"><%= renja[:title] %></span>
            </span>
          <% end %>
        </li>
      <% end %>
    </ul>
  </div>
</li>
