<h1>Kota Madiun</h1>

<table>
  <thead>
    <tr>
      <th>Nama Lembaga</th>
      <th>Kode Lembaga</th>
      <th>Tahun</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lembagas.each do |lembaga| %>
      <tr>
        <td><%= lembaga.nama_lembaga %></td>
        <td><%= lembaga.kode_lembaga %></td>
        <td><%= lembaga.tahun %></td>
        <td><%= link_to "Show", lembaga %></td>
        <td><%= link_to "Edit", edit_lembaga_path(lembaga) %></td>
        <td><%= link_to "Destroy", lembaga, method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New Lembaga", new_lembaga_path %>
