bootstrap.Modal.getInstance($('#form-usulan')).hide()
$('#form-usulan').on('hidden.bs.modal', function () {
  bootstrap.Modal.getInstance($('#form-usulan')).dispose()
  Swal.fire({
    title: 'Usulan berhasil diupdate',
    icon: 'success',
    confirmButtonText: 'Yes',
  }).then(() => {
    Turbolinks.visit(window.location, { action: "replace" })
  })
})
