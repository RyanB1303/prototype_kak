<p id="notice"><%= notice %></p>

<h1>Pagus</h1>

<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Uang</th>
      <th>Tipe</th>
      <th>Satuan</th>
      <th>Volume</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pagus.each do |pagu| %>
      <tr>
        <td><%= pagu.item %></td>
        <td><%= pagu.uang %></td>
        <td><%= pagu.tipe %></td>
        <td><%= pagu.satuan %></td>
        <td><%= pagu.volume %></td>
        <td><%= link_to 'Show', pagu %></td>
        <td><%= link_to 'Edit', edit_pagu_path(pagu) %></td>
        <td><%= link_to 'Destroy', pagu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pagu', new_pagu_path %>
