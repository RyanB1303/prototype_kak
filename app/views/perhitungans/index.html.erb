<p id="notice"><%= notice %></p>

<h1>Perhitungans</h1>

<table>
  <thead>
    <tr>
      <th>Koefisien</th>
      <th>Volume</th>
      <th>Satuan</th>
      <th>Harga</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @perhitungans.each do |perhitungan| %>
      <tr>
        <td><%= perhitungan.koefisien %></td>
        <td><%= perhitungan.volume %></td>
        <td><%= perhitungan.satuan %></td>
        <td><%= perhitungan.harga %></td>
        <td><%= link_to 'Show', perhitungan %></td>
        <td><%= link_to 'Edit', edit_perhitungan_path(perhitungan) %></td>
        <td><%= link_to 'Destroy', perhitungan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Perhitungan', new_perhitungan_path %>
