<p id="notice"><%= notice %></p>

<h1>Permasalahan Opds</h1>

<table>
  <thead>
    <tr>
      <th>Permasalahan</th>
      <th>Kode opd</th>
      <th>Tahun</th>
      <th>Status</th>
      <th>Isu strategis opd</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @permasalahan_opds.each do |permasalahan_opd| %>
      <tr>
        <td><%= permasalahan_opd.permasalahan %></td>
        <td><%= permasalahan_opd.kode_opd %></td>
        <td><%= permasalahan_opd.tahun %></td>
        <td><%= permasalahan_opd.status %></td>
        <td><%= permasalahan_opd.isu_strategis_opd %></td>
        <td><%= link_to 'Show', permasalahan_opd %></td>
        <td><%= link_to 'Edit', edit_permasalahan_opd_path(permasalahan_opd) %></td>
        <td><%= link_to 'Destroy', permasalahan_opd, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Permasalahan Opd', new_permasalahan_opd_path %>
