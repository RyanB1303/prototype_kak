<div class="row">
  <div class="col-12" data-controller="filter-render">
    <nav aria-label="breadcrumb" class="d-none d-md-inline-block mb-3">
      <ol class="breadcrumb breadcrumb-dark breadcrumb-transparent mb-0">
        <li class="breadcrumb-item">
          <%= link_to root_path do %>
            <i class="fas fa-home"></i>
          <% end %>
        </li>
        <li class="breadcrumb-item">Pohon Kinerja</li>
        <li class="breadcrumb-item active" aria-current="page">Kota</li>
      </ol>
    </nav>
    <div class="card border-0 shadow" id="kotak_usulan" data-filter-render-target="results">
      <div class="card-header">
        <div class="row align-items-center">
          <div class="col">
            <h2 class="fs-5 fw-bold mb-0">Pohon Kinerja Kota</h2>
          </div>
        </div>
      </div>
      <div class="card-body">
        <div id="pokin" class="pohon-kinerja"></div>
        <%= link_to pdf_kota_pohon_kinerja_index_path, class: "m-3 btn btn-warning w-100", target: "blank" do %>
          <i class="fa fa-edit" data-bs-toggle="tooltip" data-bs-placement="bottom"></i>
          <span>Cetak GAMBAR Pohon Kinerja</span>
        <% end %>
      </div>
    </div>
  </div>
</div>
