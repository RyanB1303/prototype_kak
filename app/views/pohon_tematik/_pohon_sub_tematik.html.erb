<li>
  <div class="tf-nc" style="width: 450px;">
    <%= render 'pohon_tematik/item_pohon_sub_tematik', pohon: pohon %>
  </div>
  <ul id="<%= "opd_tematik_#{pohon.object_id}" %>">
    <% if local_assigns[:opds] %>
      <% opds.each do |opd| %>
        <%= render partial: 'pohon_tematik/pohon_opd_tematik', locals: {
          pohon: opd,
          strategics: strategics.select { |str| str.pohon_ref_id == opd.id },
          tacticals: tacticals,
          operationals: operationals } %>
      <% end %>
    <% end %>
  </ul>
</li>
