<li>
  <% unless pohon.pohonable.nil? %>
  <div class="tf-nc tactical-pohon" style="width: 450px;">
    <%= render 'pohon_tematik/item_pohon_tactical', pohon: pohon %>
  </div>
  <ul class='d-none' id='<%= "operational_tematik_#{pohon.object_id}" %>'>
    <% if local_assigns[:operationals] %>
      <% operationals.each do |operational| %>
        <%= render partial: 'pohon_tematik/pohon_operational_tematik', locals: { pohon: operational } %>
      <% end %>
    <% end %>
  </ul>
  <% end %>
</li>
