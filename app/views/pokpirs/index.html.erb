<p id="notice"><%= notice %></p>

<h2>Usulan Pokok Pikiran</h2>

<table>
  <thead>
    <tr>
      <th>Usulan</th>
      <th>Alamat</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pokpirs.each do |pokpir| %>
      <tr>
        <td><%= pokpir.usulan %></td>
        <td><%= pokpir.alamat %></td>
        <td><%= link_to 'Show', pokpir %></td>
        <td><%= link_to 'Edit', edit_pokpir_path(pokpir) %></td>
        <td><%= link_to 'Destroy', pokpir, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pokpir', new_pokpir_path %>
