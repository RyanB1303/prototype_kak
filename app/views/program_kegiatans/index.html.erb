<p id="notice"><%= notice %></p>

<h1>Program Kegiatan</h1>


<table>
  <thead>
    <tr>
      <th>Nama Program</th>
      <th>Nama Kegiatan</th>
      <th>Nama SubKegiatan</th>
      <th>Indikator Kinerja</th>
      <th>Target</th>
      <th>Satuan</th>
      <th>OPD</th>
      <th colspan="3">Action</th>
    </tr>
  </thead>

  <tbody>
    <% @programKegiatans.each do |prog| %>
      <tr>
        <td><%= prog.nama_program %></td>
        <td><%= prog.nama_kegiatan %></td>
        <td><%= prog.nama_subkegiatan %></td>
        <td><%= prog.indikator_kinerja %></td>
        <td><%= prog.target %></td>
        <td><%= prog.satuan %></td>
        <td><%= prog.opd.kode_opd %></td>
        <td><%= link_to 'Show', prog %></td>
        <td><%= link_to 'Edit', edit_program_kegiatan_path(prog) %></td>
        <td><%= link_to 'Destroy', prog, method: :delete, data: { confirm: "Apakah Anda Ingin Menghapus ?"} %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

