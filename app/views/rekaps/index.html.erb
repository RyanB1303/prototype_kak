<p id="notice"><%= notice %></p>

<h1>Rekaps</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rekaps.each do |rekap| %>
      <tr>
        <td><%= link_to 'Show', rekap %></td>
        <td><%= link_to 'Edit', edit_rekap_path(rekap) %></td>
        <td><%= link_to 'Destroy', rekap, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rekap', new_rekap_path %>
