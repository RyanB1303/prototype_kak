<%= render(HeaderRenjaComponent.new(title: 'Program', periode: @periode, indikator: true)) %>
<tr id="<%= dom_id(program, 'Program') %>">
  <td class="text-gray-900 text-wrap border"><%= program.kode_program %></td>
  <td class="text-gray-900 text-wrap border" colspan="3"><%= program.nama_program %></td>
  <% periode.each do |tahun| %>
    <%= render(ColIndikatorRenjaComponent.new(tahun, 'program', program)) %>
  <% end %>
</tr>
