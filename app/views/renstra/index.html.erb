<div class="row">
  <div class="col-12" data-controller="filter-render">
    <nav aria-label="breadcrumb" class="d-none d-md-inline-block mb-3">
      <ol class="breadcrumb breadcrumb-dark breadcrumb-transparent mb-0">
        <li class="breadcrumb-item">
          <%= link_to root_path do %>
            <i class="fas fa-home"></i>
          <% end %>
        </li>
        <li class="breadcrumb-item active" aria-current="page">Laporan Renstra</li>
      </ol>
    </nav>
    <%= render 'shared/filter_card', filter_url: filter_laporan_renstra_path, filter_file: 'hasil_filter_renstra', action: 'ajax:success->filter-render#append' %>
    <div class="card border-0 shadow" id="laporan_renstra" data-filter-render-target="results">
      <div class="card-header">
        <div class="row align-items-center">
          <div class="col">
            <h2 class="fs-5 fw-bold mb-0">Laporan Renstra</h2>
          </div>
        </div>
      </div>
      <div class="card-body">
        <p>Pilih Tahun dan OPD</p>
      </div>
    </div>
  </div>
</div>
<%# buat edit %>
<%= render 'shared/modal_template', modal_name: 'laporan-renstra' %>
