  <h5>Rincian Sasaran - <%= rincian.sasaran.sasaran_kinerja %></h5>
<br>
<p>
  <strong>Data terpilah:</strong>
  <%= rincian.data_terpilah %>
</p>

<p>
  <strong>Penyebab internal:</strong>
  <%= rincian.penyebab_internal %>
</p>

<p>
  <strong>Penyebab external:</strong>
  <%= rincian.penyebab_external %>
</p>

<p>
  <strong>Permasalahan umum:</strong>
  <%= rincian.permasalahan_umum %>
</p>

<p>
  <strong>Permasalahan gender:</strong>
  <%= rincian.permasalahan_gender %>
</p>

<p>
  <strong>Resiko:</strong>
  <%= rincian.resiko %>
</p>

<p>
  <strong>Lokasi pelaksanaan:</strong>
  <%= rincian.lokasi_pelaksanaan %>
</p>

<br>

<% if rincian.tahapans.any? %>
  
<div class="itungan">
  <p class="waktu-renaksi">
    <strong>Waktu yang dibutuhkan :</strong>
    <%= rincian.waktu_total == 0 ? "-" : rincian.waktu_total %> bulan
  </p>
  <p class="progress-renaksi">
    <strong>Progress Pelaksanaan :</strong>
    <%= rincian.progress_total.nan? ? 0 : number_to_percentage(rincian.progress_total, precision: 2) %>
  </p>
</div>
<% end %>
