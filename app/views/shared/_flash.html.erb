  <% unless flash.empty? %>
  <% flash.each do |key, value| %>
    <% unless value.blank? %>
      <script>
        Swal.fire({
         icon: "<%= key %>",
         title: "<%= key.capitalize %>",
         text: "<%= value %>",
         showConfirmButton: false,
         timer: 2000,
        })
      </script>
    <% end %>
  <% end %>
<% end %>
