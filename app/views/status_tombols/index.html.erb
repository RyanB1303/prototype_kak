<p id="notice"><%= notice %></p>
<h1>Status Tombols</h1>
<table>
  <thead>
    <tr>
      <th>Tombol</th>
      <th>Kode tombol</th>
      <th>Disable?</th>
      <th>Tahun</th>
      <th>Kode OPD</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @status_tombols.each do |status_tombol| %>
      <tr>
        <td><%= status_tombol.tombol %></td>
        <td><%= status_tombol.kode_tombol %></td>
        <td>
          <div class="form-check form-switch">
            <input class="form-check-input" type="checkbox" id="flexSwitchCheckDefault" <%= status_tombol.disabled ? '' : 'checked'  %>>
          </div>
          </td>
        <td><%= status_tombol.tahun %></td>
        <td><%= status_tombol.kode_opd %></td>
        <td><%= link_to 'Show', status_tombol %></td>
        <td><%= link_to 'Edit', edit_status_tombol_path(status_tombol) %></td>
        <td><%= link_to 'Destroy', status_tombol, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Status Tombol', new_status_tombol_path %>
