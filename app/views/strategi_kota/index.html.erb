<p id="notice"><%= notice %></p>

<h1>Strategi Kota</h1>

<table>
  <thead>
    <tr>
      <th>Strategi</th>
      <th>Tahun</th>
      <th>Sasaran kota</th>
      <th>Isu strategis kota</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @strategi_kota.each do |strategi_kotum| %>
      <tr>
        <td><%= strategi_kotum.strategi %></td>
        <td><%= strategi_kotum.tahun %></td>
        <td><%= strategi_kotum.sasaran_kota_id %></td>
        <td><%= strategi_kotum.isu_strategis_kota_id %></td>
        <td><%= link_to 'Show', strategi_kotum %></td>
        <td><%= link_to 'Edit', edit_strategi_kotum_path(strategi_kotum) %></td>
        <td><%= link_to 'Destroy', strategi_kotum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

