<p id="notice"><%= notice %></p>

<h1>Subdomains</h1>

<table>
  <thead>
    <tr>
      <th>Subdomain</th>
      <th>Domain</th>
      <th>Kode subdomain</th>
      <th>Keterangan</th>
      <th>Tahun</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subdomains.each do |subdomain| %>
      <tr>
        <td><%= subdomain.subdomain %></td>
        <td><%= subdomain.domain_id %></td>
        <td><%= subdomain.kode_subdomain %></td>
        <td><%= subdomain.keterangan %></td>
        <td><%= subdomain.tahun %></td>
        <td><%= link_to 'Show', subdomain %></td>
        <td><%= link_to 'Edit', edit_subdomain_path(subdomain) %></td>
        <td><%= link_to 'Destroy', subdomain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subdomain', new_subdomain_path %>
